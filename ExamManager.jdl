entity Exam {
  name String required
  numberOfStudents Integer required
  startTime Instant required
  endTime Instant required
}

entity UserAcademicInfo{
  phone String
}


dto  Exam, UserAcademicInfo with mapstruct
paginate  Exam, UserAcademicInfo with pagination
service  Exam, UserAcademicInfo with serviceClass
search  Exam, UserAcademicInfo with no
